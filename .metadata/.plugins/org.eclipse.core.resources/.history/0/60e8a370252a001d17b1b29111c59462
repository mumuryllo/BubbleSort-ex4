
public class View {

	public static void main(String[] args) {
		
      int [] vetor3 = {99, 98, 97, 96, 95, 94, 93, 92, 91};
      SortsController control = new SortsController();
      vetor3 = control.bubbleSort(vetor3);
     
     
     System.out.println("");
     System.out.println("=========================");
     for (int valor :vetor3) {
	     System.out.print(valor+" ");
		}
     System.out.println("");
     System.out.println("=========================");

	}

}
